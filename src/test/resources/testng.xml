  <parameter name="reportname" value="ForgetReport" />
  <parameter name="testname" value="BrowserTest" />
  <parameter name="key" value="false" />
    <classes>
      <class name="testcases.Login"/>
      <class name="testcases.ForgetTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
